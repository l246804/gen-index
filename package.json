{
  "name": "@rhao/gen-index",
  "type": "module",
  "version": "0.0.0",
  "description": "自动生成 index.* 文件。",
  "author": "RayHao <https://github.com/l246804>",
  "license": "MIT",
  "homepage": "https://github.com/l246804/gen-index",
  "repository": {
    "type": "git",
    "url": "https://github.com/l246804/gen-index"
  },
  "bugs": {
    "url": "https://github.com/l246804/gen-index/issues"
  },
  "keywords": [
    "gen-index",
    "index",
    "re-export",
    "export"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "gi": "./dist/bin.cjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "vite build",
    "release": "release-it",
    "cz": "cz",
    "plop": "plop",
    "prepare": "husky",
    "gi": "node ./dist/bin.cjs"
  },
  "dependencies": {
    "@rhao/types-base": "^1.3.0",
    "@rollup/pluginutils": "^5.1.0",
    "commander": "^12.0.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "unconfig": "^0.3.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2",
    "@commitlint/cli": "^18",
    "@commitlint/config-conventional": "^18",
    "@commitlint/types": "^18",
    "@release-it/conventional-changelog": "^8",
    "@rhao/plop-generators": "^1.2.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.10",
    "commitizen": "^4",
    "eslint": "^8",
    "git-cz": "^4",
    "husky": "^9",
    "nice-fns": "^1.4.1",
    "plop": "^4",
    "prettier": "^3",
    "release-it": "^17.2.1",
    "rollup": "^4.17.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1"
  }
}
